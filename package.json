{
  "name": "f9-react-scrabble",
  "version": "0.1.1",
  "private": true,
  "dependencies": {
    "@types/styled-components": "^5.1.5",
    "immutable": "4.0.0-rc.12",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-redux": "^5.0.7",
    "react-scripts-ts": "^4.0.8",
    "react-style": "^0.5.5",
    "redux": "^3.7.2",
    "redux-observable": "^0.18.0",
    "rxjs": "^5.5.8",
    "styled-components": "^5.2.1",
    "styled-components-ts": "^0.0.15"
  },
  "homepage": "https://russellf9.github.io/React-Scrabble/",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "jest --watchAll",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^26.0.19",
    "@types/node": "^14.14.14",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-redux": "^5.0.15",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "jest-cli": "^22.4.3",
    "react-test-renderer": "^17.0.1",
    "redux-devtools": "^3.4.2",
    "redux-devtools-extension": "^2.13.7",
    "ts-jest": "^22.4.2",
    "typescript": "^2.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.s?css$": "<rootDir>/test-config/assets.js",
      "\\.(jp?eg|png|gif|svg)$": "<rootDir>/test-config/assets.js"
    },
    "setupFiles": [
      "<rootDir>/test-config/shim.js",
      "<rootDir>/test-config/setup.js"
    ],
    "testMatch": [
      "**/__tests__/**.(ts|tsx|js)"
    ],
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/test-preprocessor.js"
    },
    "verbose": true,
    "testURL": "http://localhost"
  },
  "browserslist": [
    ">0.2%",
    "not ie <= 11",
    "not op_mini all"
  ]
}
